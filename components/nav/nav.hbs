<nav id=jschemer-nav aria-labelledby=jschemer-schemas-list-heading role=navigation>

  {{#unless home}}<p><a href=../index.html>Home</a></p>{{/unless}}

  <h2 id=jschemer-schemas-list-heading>Schemas</h1>

  <ul>
    {{#each schemas}}
      <li>
        <p>
          <a href='{{#if ../home}}schemas/{{filename}}{{else}}{{filename}}{{/if}}'>
            {{#if schema.title}}
              {{md schema.title 'inline'}}
            {{else}}
              Schema {{@index}}
            {{/if}}
          </a>
        </p>
      </li>
    {{/each}}
  </ul>

</nav>
